  "description": "NPM Notifier Front End",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "dev": "hjs-dev-server",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/test-helper.js \"test/**/*@(.js|.jsx)\"",
